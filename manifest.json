{
    "manifest_version": 3,
    "name": "FaceAccess",
    "version": "1.0.0",
    "description": "Website Blocker with Face Recognition",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
    ],
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "129": "logo.png"
    },
    "action": {
      "default_icon":{
        "16":"logo.png",
        "24":"logo.png",
        "32":"logo.png"
    },
    "default_title": "FaceAccess Action",
    "default_popup": "popup.html"
    }
    
}